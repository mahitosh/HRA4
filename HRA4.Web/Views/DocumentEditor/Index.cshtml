@model HRA4.Web.Models.TinyMCEModelJQuery
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_leftMenuLayout.cshtml";
}

<h3 class="page-title Margin-B-10">Document Template List <small>* Select a template to edit it.</small></h3>
<div class="clear-float"></div>
<div class="col-sm-3 modal-sidebar">
    <h5 style="background-color:#d6efff;">Template Names</h5>
    <ul>
        <li><a href="#">Survey Summary</a></li>
        <li><a href="#">Screening Mammography PCP Letter</a></li>
        <li><a href="#">MRI LMN</a></li>
        <li><a href="#">Screening Mammography Patient Letter</a></li>
        <li><a href="#">Risk Clinic New Patient PCP Letter </a></li>
        <li><a href="#">Risk Clinic Test Results PCP Letter</a></li>
        <li><a href="#">Letter of Medical Necessity</a></li>
        <li><a href="#">Consider Testing Relative Letter</a></li>
        <li><a href="#">Know Mutation Relative Letter</a></li>
    </ul>
</div>
<div class="col-sm-9 Padding-R-0">
    <h4 class="Margin-T-0">Editing Template : Template Name</h4>
        @using (Html.BeginForm())
        {
            <div class="view-doc-content"> @Html.EditorFor(model => model.Content) </div>
            <div class="pull-right">
                <br /> 
                <button type="button" class="btn btn-cancel" data-dismiss="modal">Cancel</button>
                <button type="submit" class="btn btn-success">Save Changes</button>

            </div>
        }
</div>



